module.exports=function($pending_plugins,$get_plugin){$pending_plugins.set('events',()=>{let plugin={metadata:{"name":"events","version":"0.1.0","description":"event listeners","main":"index.nt","author":"12Thanjo","dependancies":[]}};let events=new Map();;let event_map=new Map();;let event_id=-1;plugin.create=function(tag,event){event_id+=1;events.set(event_id,tag);if(event_map.has(tag)){event_map.get(tag).set(event_id,event);}else{let new_map=new Map();;new_map.set(event_id,event);event_map.set(tag,new_map);};return event_id;};plugin.emit=function(tag,data){if(event_map.has(tag)){let event_list=event_map.get(tag);;for(var[id,event]of event_list.entries()){event(data);};};};plugin.delete=function(id){let tag=events.get(id);;event_map.get(tag).delete(id);events.delete(id);};return plugin;})};