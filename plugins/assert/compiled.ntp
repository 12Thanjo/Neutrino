$pending_plugins.set('assert',()=>{let plugin={metadata:{"name":"assert","version":"0.1.0","description":"assert module","main":"index.nt","author":"12Thanjo","dependancies":["cmd"]}};let cmd=$get_plugin('cmd');let metadata=new Object(plugin.metadata);;let tests=0;let passed=0;plugin=function(title,value,condition){tests+=1;if(condition()==value){passed+=1;cmd.log("✓"+" "+title,cmd.color.green);}else{cmd.log('✘'+" "+title,cmd.color.red);};};plugin.result=function(){let result="";let color="";if(passed==tests){result="Passed";color=cmd.color.green;}else{result="Failed";color=cmd.color.red;};cmd.log("Assert test: "+(color)+(result));cmd.log("tests passed: "+(passed)+"/"+(tests),color);};plugin.metadata=metadata;return plugin;});